<tr>
  <td><%= test.title %></td>
  <td><%= test.category.title %></td>
  <td><%= test.level %></td>
  <td><%= test.questions.count %></td>
  <td><% if test.time_to_pass %> <%= test.time_to_pass %> <%= t('.seconds') %> <% end %> </td>
  <td>
    <%= button_to t('.start_test'), start_test_path(test), class: "btn btn-primary" %>
  </td>
</tr>
